mysql> SELECT MaritalStatus, AVG(NetworthInMillions) FROM Actors GROUP BY MaritalStatus ORDER BY MaritalStatus ASC;
+---------------+-------------------------+
| MaritalStatus | AVG(NetworthInMillions) |
+---------------+-------------------------+
| Married       |                100.0000 |
| Single        |                200.0000 |
+---------------+-------------------------+
2 rows in set (0.00 sec)

mysql> SELECT MaritalStatus, AVG(NetworthInMillions) AS NetWorth 
    -> FROM Actors 
    -> GROUP BY MaritalStatus 
    -> HAVING NetWorth > 150 OR NetWorth < 250;
+---------------+----------+
| MaritalStatus | NetWorth |
+---------------+----------+
| Married       | 100.0000 |
| Single        | 200.0000 |
+---------------+----------+
2 rows in set (0.00 sec)

mysql> SELECT MaritalStatus, AVG(NetworthInMillions) AS NetWorth 
    -> FROM Actors 
    -> GROUP BY MaritalStatus 
    -> HAVING MaritalStatus='Married';
+---------------+----------+
| MaritalStatus | NetWorth |
+---------------+----------+
| Married       | 100.0000 |
+---------------+----------+
1 row in set (0.00 sec)

mysql> SELECT MaritalStatus, AVG(NetworthInMillions) AS NetWorth FROM Actors WHERE MaritalStatus='Married' GROUP BY MaritalStatus;
+---------------+----------+
| MaritalStatus | NetWorth |
+---------------+----------+
| Married       | 100.0000 |
+---------------+----------+
1 row in set (0.00 sec)

